{
  "name": "volvotrucks-na",
  "private": true,
  "version": "55.0.0",
  "description": "Volvo Trucks North America",
  "engines": {
    "node": ">=20.19.5 <23",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build:dev": "cross-env SOURCEMAPS=true webpack --config ./webpack.config.js",
    "watch": "cross-env SOURCEMAPS=true webpack --watch --config ./webpack.config.js",
    "dev": "aem up",
    "dev:ca": "aem up --url https://main--volvotrucks-ca--volvogroup.aem.live/en-ca/",
    "dev:mx": "aem up --url https://main--volvotrucks-mx--volvogroup.aem.page",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'common/**/*.css' 'blocks/**/*.css' 'styles/*.css' 'templates/**/*.css'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js:fix": "eslint . --fix",
    "lint:css:fix": "stylelint 'common/**/*.css' 'blocks/**/*.css' 'styles/*.css' 'templates/**/*.css' --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "e2e": "npx playwright test --config=e2e/configs/playwright.config.js",
    "e2e:local": "cross-env LOCAL_DEV=TRUE npx playwright test --config=e2e/configs/playwright.config.js",
    "e2e:branch": "cross-env BRANCH=$BRANCH npx playwright test --config=e2e/configs/playwright.config.js",
    "e2e:base": "cross-env BASE_URL=$BASE_URL npx playwright test --config=e2e/configs/playwright.config.js",
    "e2e:ui": "npx playwright test --ui --config=e2e/configs/playwright.config.js",
    "e2e:ui:local": "cross-env LOCAL_DEV=TRUE npx playwright test --ui --config=e2e/configs/playwright.config.js",
    "e2e:report": "npx playwright show-report e2e-report",
    "e2e:smoke": "npx playwright test --config=e2e/configs/playwright.config.js --grep \"@smoke\"",
    "prepare": "husky",
    "postupdate": "copyfiles -u 3 'node_modules/video.js/dist/video.min.js' 'node_modules/video.js/dist/video-js.min.css' scripts/videojs"
  },
  "lint-staged": {
    "*.js": "npm run lint:js",
    "*.css": "npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aemsites/volvotrucks-na"
  },
  "author": "Adobe",
  "license": "UNLICENSED",
  "homepage": "https://github.com/adobe/helix-project-boilerplate#readme",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.0",
    "cheerio": "^1.1.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "html-webpack-plugin": "^5.6.3",
    "lint-staged": "^16.2.6",
    "mini-css-extract-plugin": "^2.9.2",
    "playwright": "^1.56.1",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "video.js": "8.23.4",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@volvo/vcdk": "^9.14.7",
    "@volvo/vcdk-assets": "1.1.0",
    "@volvo/vcdk-tokens": "1.1.0"
  }
}
