{
  "name": "volvotrucks-na",
  "private": true,
  "version": "34.0.0",
  "description": "Volvo Trucks North America",
  "engines": {
    "node": ">=v20.15.1 <23",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "aem up",
    "dev:ca": "AEM_PAGES_URL=https://main--volvotrucks-ca--volvogroup.aem.page/en-ca/ aem up",
    "dev:mx": "AEM_PAGES_URL=https://main--volvotrucks-mx--volvogroup.aem.page aem up",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'common/**/*.css' 'blocks/**/*.css' 'styles/*.css' 'templates/**/*.css'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js:fix": "eslint . --fix",
    "lint:css:fix": "stylelint 'common/**/*.css' 'blocks/**/*.css' 'styles/*.css' 'templates/**/*.css' --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "prepare": "husky",
    "postupdate": "copyfiles -u 3 'node_modules/video.js/dist/video.min.js' 'node_modules/video.js/dist/video-js.min.css' 'node_modules/videojs-contrib-dash/dist/videojs-dash.min.js' scripts/videojs && copyfiles -u 5 'node_modules/dashjs/dist/legacy/umd/dash.all.min.js' scripts/videojs"
  },
  "lint-staged": {
    "*.js": "npm run lint:js",
    "*.css": "npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aemsites/volvotrucks-na"
  },
  "author": "Adobe",
  "license": "UNLICENSED",
  "homepage": "https://github.com/adobe/helix-project-boilerplate#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.7",
    "stylelint": "16.12.0",
    "stylelint-config-standard": "36.0.1",
    "video.js": "8.17.3"
  },
  "dependencies": {
    "dashjs": "^5.0.1",
    "videojs-contrib-dash": "^5.1.1"
  }
}
